<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg"
    tools:context=".AnotherLogin">

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="412dp"
        android:layout_height="247dp"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/bgfour" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="144dp"
        android:layout_height="167dp"
        android:layout_marginTop="70dp"
        android:layout_marginBottom="139dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView6"
        app:srcCompat="@drawable/objectthree" />

    <RelativeLayout
        android:layout_width="409dp"
        android:layout_height="353dp"
        android:layout_marginStart="1dp"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="34dp"
        android:layout_marginEnd="1dp"
        android:layout_marginRight="1dp"
        android:layout_marginBottom="1dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView6">

        <TextView
            android:id="@+id/textView16"
            android:layout_width="135dp"
            android:layout_height="wrap_content"
            android:text="LOGIN"
            android:textColor="@android:color/black"
            android:textSize="36sp"
            android:textStyle="bold" />

        <ImageView
            android:id="@+id/imageView8"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView16"
            android:scaleType="fitXY"
            android:clickable="true"
            app:srcCompat="@drawable/btnsecndry" />
        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/imageView8"
            android:scaleType="fitXY"
            android:layout_marginTop="18dp"
            app:srcCompat="@drawable/btnsecndry" />
        <ImageView
            android:id="@+id/imageView10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/imageView9"
            android:scaleType="fitXY"
            android:layout_marginTop="18dp"
            app:srcCompat="@drawable/btnsecndry" />
        <ImageView
            android:id="@+id/imageView11"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/imageView10"
            android:scaleType="fitXY"
            android:layout_marginTop="18dp"
            app:srcCompat="@drawable/btnsecndry" />

        <ImageView
            android:id="@+id/imageView13"
            android:layout_width="56dp"
            android:layout_height="33dp"
            android:layout_alignTop="@+id/imageView8"
            android:layout_alignLeft="@+id/imageView8"
            android:layout_alignBottom="@+id/imageView8"
            app:srcCompat="@drawable/mobile" />
        <ImageView
            android:id="@+id/imageView14"
            android:layout_width="56dp"
            android:layout_height="33dp"
            android:layout_alignTop="@+id/imageView9"
            android:layout_alignLeft="@+id/imageView9"
            android:layout_alignBottom="@+id/imageView9"
            app:srcCompat="@drawable/fb" />
        <ImageView
            android:id="@+id/imageView15"
            android:layout_width="56dp"
            android:layout_height="33dp"
            android:layout_alignTop="@+id/imageView10"
            android:layout_alignLeft="@+id/imageView10"
            android:layout_alignBottom="@+id/imageView10"
            app:srcCompat="@drawable/google" />
        <ImageView
            android:id="@+id/imageView16"
            android:layout_width="56dp"
            android:layout_height="33dp"
            android:layout_alignTop="@+id/imageView11"
            android:layout_alignLeft="@+id/imageView11"
            android:layout_alignBottom="@+id/imageView11"
            app:srcCompat="@drawable/mail" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/imageView8"
            android:layout_alignTop="@+id/imageView8"
            android:layout_alignRight="@+id/imageView8"
            android:layout_alignBottom="@+id/imageView8"
            android:text="Enter your mobile number"
            android:layout_marginLeft="70dp"
            android:gravity="center|start"
            android:textColor="@android:color/black"/>
        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/imageView9"
            android:layout_alignTop="@+id/imageView9"
            android:layout_alignRight="@+id/imageView9"
            android:layout_alignBottom="@+id/imageView9"
            android:text="Continue with Facebook"
            android:layout_marginLeft="70dp"
            android:gravity="center|start"
            android:textColor="@android:color/black"/>
        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/imageView10"
            android:layout_alignTop="@+id/imageView10"
            android:layout_alignRight="@+id/imageView10"
            android:layout_alignBottom="@+id/imageView10"
            android:text="Continue with Google"
            android:layout_marginLeft="70dp"
            android:gravity="center|start"
            android:textColor="@android:color/black"/>
        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/imageView11"
            android:layout_alignTop="@+id/imageView11"
            android:layout_alignRight="@+id/imageView11"
            android:layout_alignBottom="@+id/imageView11"
            android:text="Continue with Mail"
            android:layout_marginLeft="70dp"
            android:gravity="center|start"
            android:textColor="@android:color/black"/>

        <TextView
            android:id="@+id/textView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/imageView11"
            android:text="@string/your_text"
            android:layout_margin="54dp"
            android:layout_centerInParent="true" />

    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>